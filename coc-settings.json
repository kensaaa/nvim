{
  "signature.enable": false,
  "suggest.enablePreview": false,
  "suggest.floatEnable": true,
  "suggest.triggerCompletionWait": 50,
  "suggest.maxCompleteItemCount": 16,
  "tailwindCSS.emmetCompletions": true,
  "tailwindCSS.headwind.runOnSave": true,
  "tailwindCSS.jsLanguages": [
      "javascript",
      "javascriptreact",
      "reason",
      "typescriptreact"
  ],
  "typescript.preferences.quoteStyle": "single",
  "javascript.preferences.quoteStyle": "single",
  "typescript.suggest.completeFunctionCalls": false,
  "javascript.suggest.completeFunctionCalls": false,
  "prettier.tabWidth": 4,
  "prettier.singleQuote": true,
  "coc.preferences.formatOnSaveFiletypes": ["css", "svelte","javascript", "typescript","typescriptreact"]
}
